<div class= "wrapper">
    <%= render "layouts/sidenav" %>
    <div id="content-wrapper">
        <div class= "col-lg-12">
            <h4 class= "page-header"> Medical File</h4>
        </div>
        <div class= "col-lg-3">
            <%= render "layouts/patientprofile" %>
        </div>
        <div class="column">
            <div class= "col-lg-7 panel-outer-header">
                <h5>Vitals Stats Recorded: </h5>
                <div class="panel panel-default panel-patient-vitals">
                    <div class="panel-body">
                        <p>Temperature:</p>
                        <p>Heart Rate:</p>
                        <p>Blood Pressure:</p>
                    </div>
                </div>
            </div>
            <div class= "col-lg-7 panel-outer-header">
                <h5>Allergies </h5>
                <div class="panel panel-default panel-patient-allergies">
                    <div class="panel-body">
                            <p> Drug Allergies:</p>
                            <p> Non Drug Allergies:</p>
                    </div>
                </div>
            </div>
            <div class= "col-lg-7 panel-outer-header">
                <h5>Current Medication & Supplements </h5>
                <div class="panel panel-default panel-patient-allergies">
                    <div class="panel-body">
                        Basic panel example
                    </div>
                </div>
            </div>
        </div>
        <div align="right">
            <%= link_to "Back to patients listing", patients_path %>
        </div>
    </div>
    
</div>
